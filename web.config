<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="ClassSelectionV2">
                    <match url=".*" />
                    <conditions>
                        <add input="{REQUEST_URI}" pattern="^" />
                        <add input="{REQUEST_URI}" pattern="static/.*" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="http://localhost:8000/ProjectMIS307v2/{R:0}" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
